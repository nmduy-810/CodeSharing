          <li></li>
          <li></li>
          <li></li>
          <li></li>
          <li></li>
        </ul>
      </div>
    </div>
    <!-- site wrapper -->
    <div class="site-wrapper">
      <div class="main-overlay"></div>
      <!-- header -->
      @if (IsSectionDefined("customHeader"))
      {
        @await RenderSectionAsync("customHeader")
      }
      else
      {
        @await Component.InvokeAsync("Header", "Default")
      }
      <!-- section main content -->
      <section class="main-content">
        @RenderBody()
      </section>
      
      <!-- footer -->
      @await Component.InvokeAsync("Footer", "Default")
    </div>
    <!-- end site wrapper -->
    <!-- search popup area -->
    <partial name="_SearchPopup" />
    <!-- canvas menu -->
    <div class="canvas-menu d-flex align-items-end flex-column">
      <!-- close button -->
      <button type="button" class="btn-close" aria-label="Close"></button>
      <!-- logo -->
      <div class="logo">
        <img src="~/images/logo.svg" alt="Katen" />
      </div>
      <!-- menu -->
      <nav>
        <ul class="vertical-menu">
          <li class="active">
            <a href="index.html">Home</a>
            <ul class="submenu">

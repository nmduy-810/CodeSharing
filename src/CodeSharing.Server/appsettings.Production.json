{
    "ConnectionStrings": {
        "DefaultConnection": "Data Source = localhost; Initial Catalog = CodeSharing; User Id = sa; Password = someThingComplicated1234;"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information"
        },
        "WriteTo": [
            {
                "Name": "Async",
                "Args": {
                    "configure": [
                        {
                            "Name": "File",
                            "Args": {
                                "path": "logs/daily-.txt",
                                "rollingInterval": "Day",
                                "retainedFileCountLimit": 7,
                                "buffered": true
                            }
                        }
                    ]
                }
            }
        ]
    }
}